{
	"plugins": [
		"lodash",
		"@babel/proposal-export-default-from",
		"@babel/proposal-class-properties",
		"@babel/proposal-optional-catch-binding",
		["@babel/proposal-object-rest-spread", { "useBuiltIns": true }]
	],
	"env": {
		"node": {
			"presets": [["@babel/env", {
				"targets": {
				  "node": "8"
				}
			}]]
		}
	}
}