{
  "name": "react-fetch-hooks",
  "version": "2.0.1",
  "description": "A set of react hooks to work with the fetch API and gracefully parse & deal with HTTP errors",
  "keywords": [
    "react",
    "hooks",
    "fetch",
    "lazy"
  ],
  "author": "Chad Lee <npm@chadly.net>",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "scripts": {
    "compile": "rimraf es lib && cross-env BABEL_ENV=es babel src -d es && cross-env BABEL_ENV=node babel src -d lib",
    "lint": "eslint src test",
    "test": "cross-env BABEL_ENV=node mocha --recursive --require test/babel-register"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chadly/react-fetch-hooks.git"
  },
  "dependencies": {
    "lodash": "4.x"
  },
  "peerDependencies": {
    "react": ">= 16.8 < 17"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/plugin-proposal-optional-catch-binding": "^7.2.0",
    "@babel/preset-env": "^7.4.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "babel-plugin-lodash": "^3.3.4",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-civicsource": "^11.1.0",
    "jsdom": "^14.0.0",
    "mocha": "^6.1.3",
    "prettier": "^1.17.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hooks-testing-library": "^0.4.1",
    "rimraf": "^2.6.3"
